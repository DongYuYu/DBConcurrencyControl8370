class Transaction(tid: Int, s: Schedule) extends Thread
{
	override def run()
	{
		for(i < s.indices) println(i)
	}
}

object TransactionTest extends App
{
	val s1 = new Schedule (List ( (r, 0, 0), (r, 1, 0), (w, 0, 0), (w, 1, 0) ))
	val s2 = new Schedule (List ( (r, 0, 0), (r, 1, 1), (w, 0, 0), (w, 1, 1) ))

	val t1 = new Transaction(1, s1);
	val t1 = new Transaction(2, s2);
	
}